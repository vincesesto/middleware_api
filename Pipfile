[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[packages]
fastapi = "*"
uvicorn = "*"
lxml = "*"
httpx = "*"

[dev-packages]

[requires]
python_version = "3.11"
python_full_version = "3.11.9"

[scripts]
start = "uvicorn app.main:app --host 0.0.0.0 --port 8000"
dev = "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000"
